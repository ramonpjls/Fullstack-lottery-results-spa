generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model LotteryResult {
  id              Int      @id @default(autoincrement())
  company         String
  game_name       String
  date            DateTime
  winning_numbers String[]
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt

  @@unique([company, game_name, date])
}